[user]
  email = greg.lazarev@gmail.com
[alias]
  br = branch
  dc = diff --cached
  l = log --pretty=colored
  rc = rebase --continue
  rebase-back = "!f() { git rebase -i HEAD~$1; }; f"
  ss = stash show -u
  rc = rebase --continue
[pretty]
  colored = format:%Cblue%h %Cgreen[%cr] %Creset(%an) %s
